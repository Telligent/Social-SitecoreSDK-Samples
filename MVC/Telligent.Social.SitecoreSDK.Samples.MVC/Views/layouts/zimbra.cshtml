                        <li>@Html.ActionLink("Blogs", "Blog", "ListBlogs")</li>
                        <li>@Html.ActionLink("Forums", "Forum", "ListForums")</li>
                        *@
                        <li><a href="~/mvccommunity/blogs">Blogs</a></li>
                        <li><a href="~/mvccommunity/forums">Forums</a></li>
                    </ul>
                </div>
                <!--/.nav-collapse -->
