@using Telligent.Social.SitecoreSDK.Samples.MVC
@using Sitecore.Mvc.Presentation

<!DOCTYPE html>
<html>
<head>
  <title>Community Server SDK Sitecore Examples</title>
  @Html.Sitecore().VisitorIdentification()
